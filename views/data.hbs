                <td>{{id}}</td>
                <td>{{name}}</td>
                <td>{{aadhar}}</td>
                <td>{{mobile}}</td>
                <td>{{address}}</td>
                <td>{{phc}}</td>
            </tr>
        {{/each}}
        
    </table>
</body>
</html>