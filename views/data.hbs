    <h1>Vaccination Data</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Aadhar Number</th>
            <th>Mobile Number</th>
            <th>Address</th>
            <th>PHC</td>
        </tr>
        {{#each data}}
